    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "pug-lint": "^2.7.0",
    "sort-package-json": "^2.4.1"
  },
  "peerDependencies": {
    "pug-lint": "^2.7.0"
  },
  "engines": {
    "node": ">=16"
